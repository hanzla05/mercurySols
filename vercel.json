{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/register", "dest": "/api/" },
      { "src": "/login", "dest": "/api/index.js" },
      { "src": "/users", "dest": "/api/index.js" },
      { "src": "/users/(.*)", "dest": "/api/index.js" }
    ]
  }
  